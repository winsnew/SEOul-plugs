{
    "name": "soojo_dev/autoseo",
    "description": "Laravel package for automatic SEO generation",
    "type": "library",
    "autoload": {
        "psr-4": {
            "AutoSEO\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AutoSEO\\Providers\\AutoSEOServiceProvider"
            ],
            "aliases": {
                "AutoSEO": "AutoSEO\\Facades\\AutoSEO"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}